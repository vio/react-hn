{
  "name": "react-hn",
  "version": "1.4.12",
  "description": "React-powered frontend for Hacker News using its Firebase API",
  "author": "Jonny Buchanan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/insin/react-hn.git"
  },
  "scripts": {
    "build": "npm run lint && cp node_modules/sw-toolbox/sw-toolbox.js public/sw-toolbox.js && nwb build && npm run precache",
    "check:perf": "mkdir -p stats && lighthouse http://localhost:8080 --output json --perf --output-path ./stats/lighthouse.json",
    "lint": "eslint src",
    "lint:fix": "eslint --fix .",
    "start": "nwb serve",
    "precache": "sw-precache --root=public --config=sw-precache-config.json"
  },
  "dependencies": {
    "events": "1.1.0",
    "firebase": "2.4.2",
    "firetruck.js": "0.1.1",
    "history": "2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-router": "2.4.0",
    "react-timeago": "3.0.0",
    "reactfire": "0.7.0",
    "scroll-behavior": "0.5.0",
    "setimmediate": "1.0.4"
  },
  "devDependencies": {
    "eslint-config-jonnybuchanan": "2.0.3",
    "lighthouse": "^2.5.0",
    "nwb": "0.8.1",
    "sw-precache": "^3.1.1",
    "sw-toolbox": "^3.1.1",
    "stats-webpack-plugin": "^0.6.1"
  }
}
